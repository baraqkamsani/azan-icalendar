{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*",
      ],
    },
    "types": [
      "@cloudflare/workers-types",
    ]
  },
  "include": [
    ".astro/types.d.ts",
    "**/*",
  ],
  "exclude": [
    "dist",
    "node_modules",
  ]
}